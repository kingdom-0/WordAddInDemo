<?xml version="1.0" encoding="UTF-8"?>

<customUI onLoad="Ribbon_Load" xmlns="http://schemas.microsoft.com/office/2006/01/customui">
  <ribbon>
    <tabs>
      <tab idMso="TabAddIns"
           getKeytip="GetControlKeytip"
           label="测试Tab"
           getVisible="GetControlVisible">
        <group id="Group1"
               getImage="GetImage"
               getKeytip="GetControlKeytip"
               getScreentip="GetControlScreentip"
               getSupertip="GetControlSupertip"
               getVisible="GetControlVisible"
               getLabel="GetGroupLabel">
          <menu id="MenuAddBasicInfo" screentip="添加基本数据"
                label="添加基本数据" getImage="GetImage">
            <button id="BtnAddText"
                    onAction="OnButtonActionCallback"
                    getImage="GetImage"
                    getDescription="GetControlDescription"
                    getEnabled="GetControlEnabled"
                    getLabel="GetButtonLabel"
                    getKeytip="GetControlKeytip"
                    getSupertip="GetControlSupertip"
                    getVisible="GetControlVisible"
                    getShowImage="GetControlShowImage"
                    getShowLabel="GetControlShowLabel"
                    getScreentip="GetControlScreentip" />
            <button id="BtnAddTable" screentip="在当前鼠标位置添加表格" label="添加表格"
                    onAction="OnButtonActionCallback" getImage="GetImage" />
            <button id="BtnAddImage" label="添加图片" onAction="OnButtonActionCallback"
                    getImage="GetImage" />
          </menu>
          <menu id="MenuAddWinFormControl"
                getScreentip="GetControlScreentip"
                getDescription="GetControlDescription"
                getEnabled="GetControlEnabled"
                getKeytip="GetControlKeytip"
                getShowImage="GetControlShowImage"
                getShowLabel="GetControlShowLabel"
                getLabel="GetMenuLabel"
                getImage="GetImage"
                getVisible="GetControlVisible"
                getSupertip="GetControlSupertip"
                getSize="GetControlSize">
            <button id="BtnAddDatePicker" label="添加DatePicker"
                    getImage="GetImage" onAction="OnButtonActionCallback" />
            <button id="BtnAddRichText" label="添加RichText" getImage="GetImage"
                    onAction="OnButtonActionCallback" />
            <button id="BtnAddDropDownList" label="添加DropDownList"
                    getImage="GetImage" onAction="OnButtonActionCallback" />
          </menu>
          <separator id="separator2"
                     getVisible="GetControlVisible" />
          <box id="box1" boxStyle="horizontal"
               getVisible="GetControlVisible">
            <editBox id="TextInputField"
                     getEnabled="GetControlEnabled"
                     getKeytip="GetControlKeytip"
                     getLabel="GetEditBoxLabel"
                     getScreentip="GetControlScreentip"
                     getShowImage="GetControlShowImage"
                     getShowLabel="GetControlShowLabel"
                     getSupertip="GetControlSupertip"
                     getImage="GetImage"
                     onChange="OnEditBoxTextChange"
                     getText="GetControlText"
                     getVisible="GetControlVisible" />
          </box>
        </group>
        <group id="group2" label="下拉列表">
          <comboBox id="MyComboBox"
                    getEnabled="GetControlEnabled"
                    getItemCount="GetControlItemCount"
                    getImage="GetImage"
                    getItemID="GetControlItemId"
                    getItemLabel="GetControlItemLabel"
                    getKeytip="GetControlKeytip"
                    getLabel="GetComboBoxLabel"
                    getText="GetControlText"
                    getVisible="GetControlVisible"
                    onChange="OnComboBoxChange"
                    getItemImage="GetComboBoxItemImage"
                    getItemScreentip="GetControlItemScreentip"
                    getItemSupertip="GetControlItemSupertip"
                    getScreentip="GetControlScreentip"
                    getSupertip="GetControlSupertip"
                    getShowImage="GetControlShowImage"
                    getShowLabel="GetControlShowLabel" />

          <dropDown id="MyDropDown"
                    getImage="GetImage"
                    getEnabled="GetControlEnabled"
                    getItemCount="GetControlItemCount"
                    getItemID="GetControlItemId"
                    getItemImage="GetDropdownItemImage"
                    getItemLabel="GetControlItemLabel"
                    getItemScreentip="GetControlItemScreentip"
                    getItemSupertip="GetControlItemSupertip"
                    getKeytip="GetControlKeytip"
                    getLabel="GetDropDownListLabel"
                    getScreentip="GetControlScreentip"
                    getShowImage="GetControlShowImage"
                    getShowLabel="GetControlShowLabel"
                    getVisible="GetControlVisible"
                    getSupertip="GetControlSupertip"
                    getSelectedItemID="GetControlSelectedItemId" />
          <separator id="separator1" />
          <splitButton id="MySplitButton"
                       getEnabled="GetControlEnabled"
                       getKeytip="GetControlKeytip"
                       getShowLabel="GetControlShowLabel"
                       getSize="GetControlSize"
                       getVisible="GetControlVisible">
            <button id="BtnInMySplitButton" 
                    label="测试SplitButton"
                    getImage="GetImage" />
            <menu id="MySplitButton__mnu" />
          </splitButton>
          <gallery id="MyGallery"
                   getImage="GetImage"
                   getDescription="GetControlDescription"
                   getItemCount="GetControlItemCount"
                   getItemScreentip="GetControlItemScreentip"
                   getItemSupertip="GetControlItemSupertip"
                   getLabel="GetGalleryLabel"
                   getItemImage="GetGalleryItemImage"
                   getEnabled="GetControlEnabled"
                   getItemID="GetControlItemId"
                   getItemLabel="GetControlItemLabel"
                   getKeytip="GetControlKeytip"
                   getSelectedItemID="GetControlSelectedItemId"
                   getShowImage="GetControlShowImage"
                   getShowLabel="GetControlShowLabel"
                   getSize="GetControlSize"
                   getSupertip="GetControlSupertip"
                   getVisible="GetControlVisible"
                   getScreentip="GetControlScreentip"
                   getItemWidth="GetGalleryItemWidth"
                   getItemHeight="GetGalleryItemHeight" />
        </group>
        <group id="group5" label="自定义组">
          <buttonGroup id="buttonGroup1">
            <button id="GroupBtn1" label="设置项1" getImage="GetImage" />
            <button id="GroupBtn2" label="设置项2" getImage="GetImage" />
            <button id="GroupBtn3" label="设置项3" getImage="GetImage" />
          </buttonGroup>
          <box id="box2">
            <toggleButton id="ToggleButtonSelectAll"
                          getDescription="GetControlDescription"
                          getEnabled="GetControlEnabled"
                          getKeytip="GetControlKeytip"
                          getLabel="GetToggleButtonLabel"
                          onAction="OnToggleButtonActionCallback"
                          getScreentip="GetControlScreentip"
                          getShowImage="GetControlShowImage"
                          getShowLabel="GetControlShowLabel"
                          getSize="GetControlSize"
                          getPressed="GetControlPressed"
                          getSupertip="GetControlSupertip"
                          getImage="GetImage"
                          getVisible="GetControlVisible" />
            <toggleButton id="ToggleBtnSwitchFont" label="切换文本样式"
                          onAction="OnToggleButtonActionCallback" getImage="GetImage" />
          </box>
          <box id="box3">
            <button id="BtnDisplayAreaElements" label="显示选中区域元素"
                    onAction="OnButtonActionCallback"
                    getImage="GetImage" />
          </box>
        </group>
      </tab>
      <tab id="tab2" label="测试Tab2" insertAfterMso="TabAddIns">
        <group id="group6" label="其它">
          <checkBox id="MyCheckBox"
                    getSupertip="GetControlSupertip"
                    getVisible="GetControlVisible"
                    getLabel="GetCheckBoxLabel"
                    getScreentip="GetControlScreentip"
                    getKeytip="GetControlKeytip"
                    getPressed="GetControlPressed"
                    getDescription="GetControlDescription"
                    getEnabled="GetControlEnabled" />

          <labelControl id="label2"
                        getEnabled="GetControlEnabled"
                        getLabel="GetLabelControlText"
                        getScreentip="GetControlScreentip"
                        getSupertip="GetControlSupertip"
                        getVisible="GetControlVisible"
                        getShowLabel="GetControlShowLabel" />
        </group>
      </tab>
    </tabs>
  </ribbon>
</customUI>